{
  "name": "public-api-react",
  "version": "1.0.0",
  "description": "SSR react app",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack",
    "build:server": "babel src --out-dir dist",
    "build": "npm run build:client && npm run build:server",
    "start:client": "webpack --watch",
    "start:server": "nodemon --exec babel-node src/server/index.js",
    "start": "npm-run-all --parallel start:client start:server",
    "test:unit": "jest --config ./jest.config.json"
  },
  "author": "jfigueroa",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "css-loader": "^4.3.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^1.2.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "express": "^4.17.1",
    "moment": "^2.28.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
